:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55c719603600 .scope module, "test_potential_decay" "test_potential_decay" 2 4;
 .timescale -9 -10;
v0x55c71962c1c0_0 .var "CLK", 0 0;
v0x55c71962c280_0 .var "decay_rate", 2 0;
v0x55c71962c350_0 .var "input_potential", 31 0;
v0x55c71962c450_0 .net "output_potential", 31 0, v0x55c71962bf80_0;  1 drivers
S_0x55c719603790 .scope module, "potential_decay_1" "potential_decay" 2 12, 3 5 0, S_0x55c719603600;
 .timescale -9 -10;
    .port_info 0 /INPUT 1 "CLK";
    .port_info 1 /INPUT 3 "decay_rate";
    .port_info 2 /INPUT 32 "membrane_potential";
    .port_info 3 /OUTPUT 32 "output_potential_decay";
v0x55c7195db7b0_0 .net "CLK", 0 0, v0x55c71962c1c0_0;  1 drivers
v0x55c7195dbba0_0 .var "adjusted_exponent", 7 0;
v0x55c71962bbd0_0 .net "decay_rate", 2 0, v0x55c71962c280_0;  1 drivers
v0x55c71962bc90_0 .var "exponent", 7 0;
v0x55c71962bd70_0 .var "mantissa", 22 0;
v0x55c71962bea0_0 .net "membrane_potential", 31 0, v0x55c71962c350_0;  1 drivers
v0x55c71962bf80_0 .var "output_potential_decay", 31 0;
v0x55c71962c060_0 .var "sign", 1 0;
E_0x55c7196130d0/0 .event edge, v0x55c71962bea0_0, v0x55c71962bbd0_0, v0x55c71962bc90_0, v0x55c71962c060_0;
E_0x55c7196130d0/1 .event edge, v0x55c7195dbba0_0, v0x55c71962bd70_0;
E_0x55c7196130d0 .event/or E_0x55c7196130d0/0, E_0x55c7196130d0/1;
    .scope S_0x55c719603790;
T_0 ;
    %wait E_0x55c7196130d0;
    %load/vec4 v0x55c71962bea0_0;
    %parti/s 1, 31, 6;
    %pad/u 2;
    %store/vec4 v0x55c71962c060_0, 0, 2;
    %load/vec4 v0x55c71962bea0_0;
    %parti/s 8, 23, 6;
    %store/vec4 v0x55c71962bc90_0, 0, 8;
    %load/vec4 v0x55c71962bea0_0;
    %parti/s 23, 0, 2;
    %store/vec4 v0x55c71962bd70_0, 0, 23;
    %load/vec4 v0x55c71962bbd0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %load/vec4 v0x55c71962bc90_0;
    %store/vec4 v0x55c7195dbba0_0, 0, 8;
    %jmp T_0.5;
T_0.0 ;
    %load/vec4 v0x55c71962bc90_0;
    %store/vec4 v0x55c7195dbba0_0, 0, 8;
    %jmp T_0.5;
T_0.1 ;
    %load/vec4 v0x55c71962bc90_0;
    %subi 1, 0, 8;
    %store/vec4 v0x55c7195dbba0_0, 0, 8;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0x55c71962bc90_0;
    %subi 2, 0, 8;
    %store/vec4 v0x55c7195dbba0_0, 0, 8;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0x55c71962bc90_0;
    %subi 3, 0, 8;
    %store/vec4 v0x55c7195dbba0_0, 0, 8;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %load/vec4 v0x55c71962c060_0;
    %load/vec4 v0x55c7195dbba0_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x55c71962bd70_0;
    %concat/vec4; draw_concat_vec4
    %pad/u 32;
    %store/vec4 v0x55c71962bf80_0, 0, 32;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55c719603600;
T_1 ;
    %vpi_call 2 16 "$dumpfile", "test_potential_decay.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55c719603600 {0 0 0};
    %delay 1000, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55c719603600;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c71962c1c0_0, 0, 1;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x55c71962c280_0, 0, 3;
    %pushi/vec4 1088421888, 0, 32;
    %store/vec4 v0x55c71962c350_0, 0, 32;
    %end;
    .thread T_2;
    .scope S_0x55c719603600;
T_3 ;
    %vpi_call 2 32 "$monitor", $time, " Input Potential: %b\012                     After Potential Decay: %b", v0x55c71962c350_0, v0x55c71962c450_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55c719603600;
T_4 ;
    %delay 40, 0;
    %load/vec4 v0x55c71962c1c0_0;
    %inv;
    %store/vec4 v0x55c71962c1c0_0, 0, 1;
    %jmp T_4;
    .thread T_4;
